{
  "name": "@mog13/splain",
  "version": "2.0.0",
  "description": "Small transformer library to create more dynamic and interesting language/sentances",
  "main": "splain.js",
  "scripts": {
    "build": "webpack",
    "lint-fix": "tslint --fix src/**/*.ts",
    "test": "tslint ./src/*.ts && jest --coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=!src/**/index.ts --collectCoverageFrom=utility/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mog13/Splain.git"
  },
  "author": "Morgan Owen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mog13/Splain/issues"
  },
  "homepage": "https://github.com/mog13/Splain#readme",
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/node": "^10.12.24",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "terser-webpack-plugin": "^1.2.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "bin": {
    "verify-dictionary": "bin/dictionaryValidator.js"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
